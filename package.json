  "license": "MIT",
  "engines": {
    "node": ">=12.x.x",
    "npm": ">=6.x.x"
  },
  "scripts": {
    "build": "ng build",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "ng": "ng",
    "postinstall": "ng build --prod",
    "start": "node server.js",
    "start:watch": "ng serve --port 4200",
    "test": "npm run-script lint && npm run-script test:unit && npm run-script test:e2e && npm run-script build -- --prod",
    "test:e2e": "ng e2e",
    "test:tdd": "ng test --code-coverage",
    "test:unit": "ng test --watch=false --code-coverage"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.0.2",
    "@angular/cdk": "^8.2.3",
    "@angular/cli": "^9.0.3",
