    "@angular/compiler": "^9.0.2",
    "@angular/compiler-cli": "^11.0.2",
    "@angular/core": "^9.0.2",
    "@angular/forms": "^9.0.2",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "^9.0.2",
    "@angular/platform-browser-dynamic": "^9.0.2",
    "@angular/router": "^9.0.2",
    "core-js": "^2.6.10",
    "express": "^4.17.1",
    "hammerjs": "^2.0.8",
    "rxjs": "^6.5.3",
    "rxjs-compat": "^6.5.3",
    "tslib": "^1.10.0",
    "typescript": "~3.7.5",
    "zone.js": "~0.10.2"
